{
  "name": "import-service",
  "version": "1.0.0",
  "description": "Fetches data from Open Charge Map, checks for changes, and updates the MongoDB database when necessary.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias && cp src/presentation/*.graphql dist/presentation",
    "dev": "tsx ./src/index.ts",
    "start": "node ./dist/index.js",
    "type-check": "tsc --noEmit",
    "lint-check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,ts,json,md,test.js}\"",
    "format-check": "prettier -c \"**/*.{js,ts,json,md,test.js}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "next",
      "prettier"
    ]
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@graphql-tools/schema": "^9.0.19",
    "apisauce": "^3.0.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "inversify": "^6.0.1",
    "mongoose": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "stream-json": "^1.7.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "nodemon": "^2.0.22",
    "@types/node": "^20.1.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^19.0.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.6",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "singleQuote": true
  }
}
